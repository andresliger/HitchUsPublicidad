            <sys:String x:Key="retrieveCampanias">http://localhost:8080/PublicidadCloud-web/webresources/campania/retrieveCampanias</sys:String>
            <sys:String x:Key="registrarCampania">http://localhost:8080/PublicidadCloud-web/webresources/campania/registrarCampania</sys:String>
