            <sys:String x:Key="eliminarElemento">http://localhost:8080/PublicidadCloud-web/webresources/elemento/eliminarElemento</sys:String>
            <sys:String x:Key="retrieveSegmentoDetalleCamp">http://localhost:8080/PublicidadCloud-web/webresources/segmentoDetalleC/retrieveSegmentoDetalleCamp</sys:String>
            <sys:String x:Key="registrarSegmentoDetalleCamp">http://localhost:8080/PublicidadCloud-web/webresources/segmentoDetalleC/registrarSegmentoDetalleCamp</sys:String>
            <sys:String x:Key="editarSegmentoDetalleCamp">http://localhost:8080/PublicidadCloud-web/webresources/segmentoDetalleC/editarSegmentoDetalleCamp</sys:String>
