            <sys:String x:Key="retrieveTargetEdad">http://localhost:8080/PublicidadCloud-web/webresources/targetEdad/retrieveTargetEdad</sys:String>
            <sys:String x:Key="registrarTargetEdad">http://localhost:8080/PublicidadCloud-web/webresources/targetEdad/registrarTargetEdad</sys:String>
            <sys:String x:Key="editarTargetEdad">http://localhost:8080/PublicidadCloud-web/webresources/targetEdad/editarTargetEdad</sys:String>
            <sys:String x:Key="eliminarTargetEdad">http://localhost:8080/PublicidadCloud-web/webresources/targetEdad/eliminarTargetEdad</sys:String>
