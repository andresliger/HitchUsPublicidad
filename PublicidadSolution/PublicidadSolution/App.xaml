<Application x:Class="PublicidadSolution.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:PublicidadSolution"
             DispatcherUnhandledException="Application_DispatcherUnhandledException"
             StartupUri="Inicio.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <sys:String x:Key="usuario">user_logged</sys:String>
            <sys:String x:Key="ws_retrieveUserLogged">http://localhost:8080/PublicidadCloud-web/webresources/usuario/retrieveUserLogged</sys:String>
            <sys:String x:Key="ws_retrieveUsers">http://localhost:8080/PublicidadCloud-web/webresources/usuario/retrieveUsers</sys:String>
            <sys:String x:Key="registrarUsuario">http://localhost:8080/PublicidadCloud-web/webresources/usuario/registrarUsuario</sys:String>
